#!/bin/sh
# HACK - needs to be generated automatically
export jruby_build=1.6.3
export M2_REPO=~/.m2/repository
export GEM_HOME=~/projects/amplafi-tools/ruby/gems
# END HACK

# HACK: need ~/projects/amplafi-tools/ruby/gems/bin in path as well.

java -jar ${M2_REPO}/org/jruby/jruby-complete/${jruby_build}/jruby-complete-${jruby_build}.jar -S $*
